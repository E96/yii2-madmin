{% extends 'parent.twig' %}

{% block setup %}
    {{ set(this, 'title', (model.isNewRecord ? 'Создание ' : 'Редактирование ') ~ modelTitleForms[1]) }}
{% endblock %}

{% block content %}
    {{ use('kartik/form/ActiveForm') }}
    {{ use('kartik/builder/Form') }}

    {% set form = activeForm_begin({
        type: 'horizontal',
    }) %}
    {{ html.hiddenInput('_returnUrl', returnUrl) | raw }}
    {{ form_widget({
        model: model,
        form: form,
        attributes: formElements
    }) | raw }}
    {{ activeForm_end() }}
{% endblock %}