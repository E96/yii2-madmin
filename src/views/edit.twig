{{ set(this, 'title', (model.isNewRecord ? 'Создание ' : 'Редактирование ')~ modelTitleForms[1]) }}
{{ use('e96/madmin/Asset') }}
{{ register_asset() }}

<div class="place-update">
    <h1>{{ this.title }}</h1>

    {{ use('kartik\\form\\ActiveForm') }}
    {{ use('kartik\\builder\\Form') }}

    {% set form = activeForm_begin({
        type: 'horizontal',
    }) %}
    {{ html.hiddenInput('returnUrl', returnUrl) | raw }}
    {{ form_widget({
        model: model,
        form: form,
        attributes: formElements
    }) | raw }}
    {{ activeForm_end() }}
</div>